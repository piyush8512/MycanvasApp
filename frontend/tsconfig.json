{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "types": ["react", "react-native"],
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/SafeScreen.jsx",
    "app/(auth)/_layout.jsx",
    "app/_layout.jsx",
    "app/canvas/[id].jsx",
    "components/canvas/Card",
    "components/canvas/Modal/AddCard.jsx",
    "app/(tabs)/_layout.tsx",
    "app/(tabs)/index.tsx",
    "utils/linkDetector.ts",
    "components/canvas/DraggableCard.jsx"
, "components/canvas/renderCardContent.tsx", "components/home/SpaceCard.tsx"  ]
}
